@model BookPlace.Models.Store

@{
    ViewBag.Title = "Details";
}


<div>
    <h3>Details</h3>
    <hr />

    <fieldset disabled>
        <div class="row" style="padding-bottom:5px; padding-top:5px; ">
            <div class="col-md-2">
                @Html.LabelFor(m => m.Name, htmlAttributes: new { @class = "control-label" })
            </div>
            <div class="col-md-10">
                @Html.TextBoxFor(m => m.Name, new { @class = "form-control" })
            </div>
        </div>
        <div class="row" style="padding-bottom:5px; padding-top:5px; ">
            <div class="col-md-2">
                @Html.LabelFor(m => m.City, htmlAttributes: new { @class = "control-label" })
            </div>
            <div class="col-md-10">
                @Html.TextBoxFor(m => m.City, new { @class = "form-control" })
            </div>
        </div>
        <div class="row" style="padding-bottom:5px; padding-top:5px; ">
            <div class="col-md-2">
                @Html.LabelFor(m => m.CoordinateAlt, htmlAttributes: new { @class = "control-label" })
            </div>
            <div class="col-md-10">
                @Html.TextBoxFor(m => m.CoordinateAlt, new { @class = "form-control" })
            </div>
        </div>
        <div class="row" style="padding-bottom:5px; padding-top:5px; ">
            <div class="col-md-2">
                @Html.LabelFor(m => m.CoordinateLng, htmlAttributes: new { @class = "control-label" })
            </div>
            <div class="col-md-10">
                @Html.TextBoxFor(m => m.CoordinateLng, new { @class = "form-control" })
            </div>
        </div>
        <div class="row" style="padding-bottom:5px; padding-top:5px; ">
            <div class="col-md-2">
                @Html.LabelFor(m => m.Address, htmlAttributes: new { @class = "control-label" })
            </div>
            <div class="col-md-10">
                @Html.TextBoxFor(m => m.Address, new { @class = "form-control" })
            </div>
        </div>
    </fieldset>
</div>
<br />

<p>
    @Html.Partial("_BackToListButtonPartial")
</p>
